<Window x:Class="ASDSWPFUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ASDSWPFUI"
        xmlns:miscElements="clr-namespace:ASDSWPFUI.MiscElements"
        xmlns:networkUi="clr-namespace:NetworkUI;assembly=NetworkUI"
        xmlns:networkModel="clr-namespace:ASDSWPFUI.ViewModel"
        xmlns:adornedControl="clr-namespace:AdornedControl;assembly=AdornedControl"
        xmlns:zoomAndPan="clr-namespace:ZoomAndPan;assembly=ZoomAndPan"
        xmlns:graphArea="clr-namespace:ASDSWPFUI.GraphArea"
        mc:Ignorable="d"
        Title="Main Window" 
        x:Name="mainWindow"
        MinWidth="400"
        MinHeight="150"
        Width="800"
        Height="500"
        Loaded="MainWindow_Loaded"
        >
  <Grid>
      <graphArea:GraphArea 
          x:Name="mainGraphArea"/>
  </Grid>
</Window>
